{
    "multipart": [
        { "when": { "axis": "x" }, "apply": { "model": "rustic:chain_horizontal", "y": 90 } },
        { "when": { "axis": "y" }, "apply": { "model": "rustic:chain" } },
        { "when": { "axis": "z" }, "apply": { "model": "rustic:chain_horizontal" } },
        { "when": { "axis": "x", "dangle": "true" }, "apply": { "model": "rustic:chain_dangle", "y": 90 } },
        { "when": { "axis": "y|z", "dangle": "true" }, "apply": { "model": "rustic:chain_dangle" } }
    ]
}