{
    "multipart": [
        { "when": { "axis": "x" }, "apply": { "model": "rustic:rope_horizontal", "y": 90 } },
        { "when": { "axis": "y" }, "apply": { "model": "rustic:rope" } },
        { "when": { "axis": "z" }, "apply": { "model": "rustic:rope_horizontal" } },
        { "when": { "axis": "x", "dangle": "true" }, "apply": { "model": "rustic:rope_dangle", "y": 90 } },
        { "when": { "axis": "x", "dangle": "true" }, "apply": { "model": "rustic:rope_knot", "y": 90 } },
        { "when": { "axis": "y|z", "dangle": "true" }, "apply": { "model": "rustic:rope_dangle" } },
        { "when": { "axis": "y|z", "dangle": "true" }, "apply": { "model": "rustic:rope_knot" } }
    ]
}