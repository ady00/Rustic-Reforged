{
    "multipart": [
        { "when": { "axis": "x" }, "apply": { "model": "rusticreforged:block/chain_horizontal", "y": 90 } },
        { "when": { "axis": "y" }, "apply": { "model": "rusticreforged:block/chain" } },
        { "when": { "axis": "z" }, "apply": { "model": "rusticreforged:block/chain_horizontal" } },
        { "when": { "axis": "x", "dangle": "true" }, "apply": { "model": "rusticreforged:block/chain_dangle", "y": 90 } },
        { "when": { "axis": "y|z", "dangle": "true" }, "apply": { "model": "rusticreforged:block/chain_dangle" } }
    ]
}